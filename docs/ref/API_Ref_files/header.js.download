jQuery.fn.autotab = function () {
    jQuery(this).keyup(function (e) {
        switch (e.keyCode) {
            case 9:
            case 16:
            case 20:
                return false;
                break;
            default:
                var maxlength = jQuery(this).attr('maxlength');
                var inputlength = jQuery(this).val().length;
                if (inputlength >= maxlength) {
                    jQuery(this).next('input[type="text"]').focus();
                }
                break;
        }
    });
}