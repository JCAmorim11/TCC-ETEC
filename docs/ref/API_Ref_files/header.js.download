formatarDropListIE = function (el) {
    var selectWidthDefault = $(el).css("width");

    if ($.browser.msie) {
        $(el).bind('mousedown', function () {
            $(this).css("width", "auto");
        });
        $(el).bind('change', function () {
            $(this).css("width", selectWidthDefault);
        });
        $(el).bind('blur', function () {
            $(this).css("width", selectWidthDefault);
        });
    }
}

